<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler" xmlns:d="jelly:define" xmlns:l="/lib/layout"
         xmlns:t="/lib/hudson"
         xmlns:f="/lib/form">

    <f:block>
        <h3 style="margin-top: 0px;">Manual Subscription</h3>
    </f:block>

    <f:invisibleEntry>
        <f:readOnlyTextbox default="ER" field="type"/>
    </f:invisibleEntry>

    <f:entry title="CodeCommit Repo URL" field="url" description="only Git supported">
        <f:textbox placeholder="ex: https://git-codecommit.us-west-2.amazonaws.com/v1/repos/sample-repo"/>
    </f:entry>

    <f:entry description="csv format, more details in help button" title="Subscribed branches (blank for 'any')"
             field="subscribedBranches">
        <f:textbox placeholder="ex: development, master, *feature, feature/**"/>
    </f:entry>

    <f:entry>
        <div align="right">
            <f:repeatableDeleteButton/>
        </div>
    </f:entry>
</j:jelly>
